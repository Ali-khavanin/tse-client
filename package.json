  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-ahmadi/tc/issues"
  },
  "homepage": "https://github.com/m-ahmadi/tc#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "big.js": "^5.2.2",
    "commander": "^3.0.1",
    "jalaali-js": "^1.1.0"
  }
}

