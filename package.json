{
  "name": "tse-client",
  "version": "0.1.19",
  "description": "A client for receiving stock data from the Tehran Stock Exchange (TSE).",
  "main": "index.js",
  "bin": {
    "tc": "index.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "nodemon -x node --inspect-brk index.js -w ./ -e js,json,csv",
